{
  "name": "reshift",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": "^22",
    "pnpm": "^10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint:ci": "pnpm run lint:check --cache --cache-strategy content --cache-location ~/.cache/eslint/reshift",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vueuse/core": "14.0.0",
    "@vueuse/nuxt": "14.0.0",
    "nuxt": "4.2.0",
    "vue": "3.5.22",
    "vue-router": "4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.2.0",
    "@nuxt/test-utils": "3.20.1",
    "@vitest/coverage-v8": "4.0.6",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.39.0",
    "eslint-plugin-format": "1.0.2",
    "happy-dom": "20.0.10",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "vitest": "4.0.6",
    "vue-tsc": "3.1.2"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
